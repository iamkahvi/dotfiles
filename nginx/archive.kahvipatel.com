server {
    if ($host = www.archive.kahvipatel.com) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


    if ($host = archive.kahvipatel.com) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


	listen 80;
	listen [::]:80;

	server_name www.archive.kahvipatel.com archive.kahvipatel.com;
    return 404; # managed by Certbot




}
