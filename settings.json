{
  "workbench.colorTheme": "One Dark Pro",
  "oneDarkPro.vivid": true,
  "workbench.sideBar.location": "right",
  "editor.copyWithSyntaxHighlighting": false,
  "editor.emptySelectionClipboard": true,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.gotoLocation.multipleReferences": "goto",
  "editor.gotoLocation.multipleDefinitions": "goto",
  "editor.gotoLocation.multipleDeclarations": "goto",
  "editor.gotoLocation.multipleImplementations": "goto",
  "editor.gotoLocation.multipleTypeDefinitions": "goto",
  "editor.inlineSuggest.enabled": true,
  "editor.snippetSuggestions": "top",
  "editor.detectIndentation": false,
  "editor.formatOnPaste": true,
  "editor.minimap.enabled": false,
  "editor.renderWhitespace": "none",
  "editor.autoClosingBrackets": "always",
  "editor.suggestSelection": "first",
  "editor.trimAutoWhitespace": false,
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": true,
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "diffEditor.renderSideBySide": false,
  "window.nativeFullScreen": true,
  "window.newWindowDimensions": "inherit",
  "window.openFoldersInNewWindow": "on",
  "terminal.integrated.inheritEnv": true,
  "git.autofetch": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "remote.SSH.localServerDownload": "always",
  "eslint.alwaysShowStatus": true,
  "editor.tabSize": 2,
  "eslint.codeAction.showDocumentation": {
    "enable": true
  },
  "eslint.format.enable": true,
  "[antlr]": {
    "editor.formatOnPaste": false
  },
  "files.associations": {
    "*.java": "java",
    "*.html.erb": "erb"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.tabSize": 2
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false,
  },
  "[scss]": {
    "editor.defaultFormatter": "mrmlnc.vscode-scss"
  },
  "[css]": {
    "editor.defaultFormatter": "vscode.css-language-features"
  },
  "[c]": {
    "editor.wordBasedSuggestions": false,
    "editor.suggest.insertMode": "replace"
  },
  "[cpp]": {
    "editor.wordBasedSuggestions": false,
    "editor.suggest.insertMode": "replace",
    "editor.formatOnSave": true
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[go]": {
    "editor.insertSpaces": false,
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    },
    "editor.autoClosingBrackets": "always"
  },
  "[lua]": {
    "editor.formatOnSave": true
  },
  "[sql]": {
    "editor.formatOnSave": false
  },
  "[python]": {
    "editor.formatOnSave": true
  },
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "files.watcherExclude": {
    "**/.bloop": true,
    "**/.metals": true,
    "**/.ammonite": true
  },
  "metals.javaHome": "/Users/kahvi/.jabba/jdk/adopt@1.11.0-11/Contents/Home",
  "github.copilot.enable": {
    "*": true,
    "yaml": false,
    "plaintext": false,
    "markdown": false
  },
  "workbench.editor.showTabs": false,
  "files.autoSave": "afterDelay",
  "remote.SSH.defaultExtensions": [
    "eamodio.gitlens",
  ],
  "shopifyGlobal.mysqlEditor": "external",
  "ruby.useLanguageServer": false,
  "ruby.lint": {
    "rubocop": false
  },
  "ruby.codeCompletion": false,
  "ruby.intellisense": false,
  "ruby.format": false,
  "[ruby]": {
    "editor.defaultFormatter": "Shopify.ruby-lsp",
    "editor.formatOnSave": true,
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.semanticHighlighting.enabled": true,
    "editor.formatOnType": true
  },
  "byesig.fold": false,
  "byesig.enabled": true,
  "byesig.opacity": 0.5,
  "byesig.showIcon": false,
  "editor.rulers": [
    120
  ],
  "git.mergeEditor": false,
  "workbench.editor.enablePreview": false,
  "explorer.compactFolders": false,
  "workbench.colorCustomizations": {
    "editorInlayHint.typeForeground": "#666666",
    "editorInlayHint.typeBackground": "#232323",
    "editorInlayHint.foreground": "#666666",
    "editorInlayHint.background": "#232323",
    "editorInlayHint.parameterForeground": "#666666",
    "editorInlayHint.parameterBackground": "#232323",
  },
  "[rust]": {
    "editor.tabSize": 4,
    "editor.formatOnSave": true,
  },
  "rust-analyzer.check.command": "clippy",
  "rust-analyzer.checkOnSave": true,
  "window.zoomLevel": 1
}
