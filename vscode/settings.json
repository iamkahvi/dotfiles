  "git.autofetch": true,
  "git.mergeEditor": false,
  "github.copilot.enable": {
    "*": true,
    "markdown": false,
    "plaintext": false,
    "scminput": false,
    "yaml": false
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "oneDarkPro.vivid": true,
  "remote.SSH.defaultExtensions": [
    "eamodio.gitlens"
