  "files.associations": {
    "*.html.erb": "erb",
    "*.java": "java"
  },
  "files.autoSave": "afterDelay",
  "files.exclude": {
    "**/.classpath": true,
    "**/.factorypath": true,
    "**/.project": true,
    "**/.settings": true
